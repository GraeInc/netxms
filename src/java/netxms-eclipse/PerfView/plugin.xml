<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            name="%view.name.HistDataChart"
            allowMultiple="true"
            icon="icons/chart_line.png"
            class="org.netxms.ui.eclipse.perfview.views.HistoricalGraphView"
            id="org.netxms.ui.eclipse.perfview.views.HistoryGraph">
      </view>
      <view
            allowMultiple="false"
            class="org.netxms.ui.eclipse.perfview.views.PredefinedGraphTree"
            icon="icons/graph_tree.png"
            id="org.netxms.ui.eclipse.perfview.views.PredefinedGraphTree"
            name="%view.name.Graphs"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="org.netxms.ui.eclipse.perfview.views.DataComparisonView"
            icon="icons/chart_bar.png"
            id="org.netxms.ui.eclipse.perfview.views.DataComparisionView"
            name="%view.name.LastValuesChart"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="org.netxms.ui.eclipse.perfview.views.TableLastValuesView"
            icon="icons/table.gif"
            id="org.netxms.ui.eclipse.perfview.views.TableLastValues"
            name="%view.name.TableLastValues"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="org.netxms.ui.eclipse.perfview.views.HistoricalDataView"
            icon="icons/data_history.gif"
            id="org.netxms.ui.eclipse.perfview.views.HistoricalDataView"
            name="%view.name.HistData"
            restorable="true">
      </view>
   </extension>

   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="org.netxms.ui.eclipse.perfview.actions.popup.dci.SimpleDciValue"
            objectClass="org.netxms.client.datacollection.SimpleDciValue">
         <action
               class="org.netxms.ui.eclipse.perfview.actions.ShowPieChart"
               icon="icons/chart_pie.png"
               id="org.netxms.ui.eclipse.perfview.popupActions.ShowPieChart_SimpleDciValue#30"
               label="%action.label.PieChart"
               menubarPath="additions">
         </action>
         <action
               class="org.netxms.ui.eclipse.perfview.actions.ShowBarChart"
               icon="icons/chart_bar.png"
               id="org.netxms.ui.eclipse.perfview.popupActions.ShowBarChart_SimpleDciValue#20"
               label="%action.label.BarChart"
               menubarPath="additions">
         </action>
         <action
               class="org.netxms.ui.eclipse.perfview.actions.ShowHistoryGraph"
               id="org.netxms.ui.eclipse.perfview.popupActions.ShowHistoryGraph_SimpleDciValue#10"
               label="%action.label.LineChart"
               icon="icons/chart_line.png"
               menubarPath="additions">
         </action>
         <action
               class="org.netxms.ui.eclipse.perfview.actions.ShowHistoryData"
               icon="icons/data_history.gif"
               id="org.netxms.ui.eclipse.perfview.popupActions.ShowHistoryData_SimpleDciValue#40"
               label="%action.label.History"
               menubarPath="additions">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="org.netxms.ui.eclipse.perfview.actions.popup.dci.TableDciValue"
            objectClass="org.netxms.client.datacollection.TableDciValue">
         <action
               class="org.netxms.ui.eclipse.perfview.actions.ShowTableLastValues"
               icon="icons/table.gif"
               id="org.netxms.ui.eclipse.perfview.actions.ShowTableLastValues_TableDciValue#10"
               label="%action.label.TableLastValue"
               menubarPath="additions">
         </action>
      </objectContribution>
      <objectContribution
            id="org.netxms.ui.eclipse.perfview.actions.popup.dci.DataCollectionItem"
            objectClass="org.netxms.client.datacollection.DataCollectionItem">
         <action
               class="org.netxms.ui.eclipse.perfview.actions.ShowPieChart"
               icon="icons/chart_pie.png"
               id="org.netxms.ui.eclipse.perfview.popupActions.ShowPieChart_DataCollectionItem"
               label="%action.label.PieChart"
               menubarPath="additions">
         </action>
         <action
               class="org.netxms.ui.eclipse.perfview.actions.ShowBarChart"
               icon="icons/chart_bar.png"
               id="org.netxms.ui.eclipse.perfview.popupActions.ShowBarChart_DataCollectionItem"
               label="%action.label.BarChart"
               menubarPath="additions">
         </action>
         <action
               class="org.netxms.ui.eclipse.perfview.actions.ShowHistoryGraph"
               id="org.netxms.ui.eclipse.perfview.popupActions.ShowHistoryGraph_DataCollectionItem"
               label="%action.label.LineChart"
               icon="icons/chart_line.png"
               menubarPath="additions">
         </action>
         <action
               class="org.netxms.ui.eclipse.perfview.actions.ShowHistoryData"
               icon="icons/data_history.gif"
               id="org.netxms.ui.eclipse.perfview.popupActions.ShowHistoryData_DataCollectionItem"
               label="%action.label.History"
               menubarPath="additions">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="org.netxms.ui.eclipse.perfview.actions.popup.dci.DataCollectionTable"
            objectClass="org.netxms.client.datacollection.DataCollectionTable">
         <action
               class="org.netxms.ui.eclipse.perfview.actions.ShowTableLastValues"
               icon="icons/table.gif"
               id="org.netxms.ui.eclipse.perfview.actions.ShowTableLastValues_DataCollectionTable"
               label="%action.label.TableLastValue"
               menubarPath="additions">
         </action>
      </objectContribution>
   </extension>

   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="org.netxms.ui.eclipse.perfview.propertypages.PerfTab"
            id="org.netxms.ui.eclipse.perfview.propertypages.PerfTab#100"
            name="%page.name.PerfTab">
         <enabledWhen>
            <instanceof
                  value="org.netxms.client.datacollection.DataCollectionItem">
            </instanceof>
         </enabledWhen>
      </page>
      <page
            class="org.netxms.ui.eclipse.perfview.propertypages.General"
            id="org.netxms.ui.eclipse.perfview.propertypages.General#1"
            name="%page.name.General">
         <enabledWhen>
            <instanceof
                  value="org.netxms.ui.eclipse.perfview.ChartConfig">
            </instanceof>
         </enabledWhen>
      </page>
      <page
            class="org.netxms.ui.eclipse.perfview.propertypages.DataSources"
            id="org.netxms.ui.eclipse.perfview.propertypages.DataSources#10"
            name="%page.name.DataSources">
         <enabledWhen>
            <instanceof
                  value="org.netxms.ui.eclipse.perfview.ChartConfig">
            </instanceof>
         </enabledWhen>
      </page>
      <page
            class="org.netxms.ui.eclipse.perfview.propertypages.PredefinedGraph"
            id="org.netxms.ui.eclipse.perfview.propertypages.PredefinedGraph#0"
            name="%page.name.PredefGraph">
         <enabledWhen>
            <instanceof
                  value="org.netxms.ui.eclipse.perfview.PredefinedChartConfig">
            </instanceof>
         </enabledWhen>
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            description="%command.description.PredefGraphs"
            id="org.netxms.ui.eclipse.perfview.commands.open_predef_graph_tree"
            name="%command.name.PredefGraphs">
      </command>
      <command
            id="org.netxms.ui.eclipse.perfview.commands.show_graph_filter"
            name="Show filter">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="org.netxms.ui.eclipse.action_set.predefined_graphs"
            label="%actionSet.label.Graphs"
            visible="true">
         <action
               class="org.netxms.ui.eclipse.perfview.actions.OpenPredefinedGraphsTree"
               definitionId="org.netxms.ui.eclipse.perfview.commands.open_predef_graph_tree"
               icon="icons/graph_tree.png"
               id="org.netxms.ui.eclipse.perfview.action.open_predef_graph_tree"
               label="%action.label.PredefGraphs"
               menubarPath="view/primary"
               style="push"
               toolbarPath="view/additions">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.netxms.ui.eclipse.objectview.tabs">
      <tab
            class="org.netxms.ui.eclipse.perfview.objecttabs.PerformanceTab"
            icon="icons/chart_line.png"
            id="org.netxms.ui.eclipse.perfview.objecttabs.PerformanceTab"
            name="%tab.name.Performance"
            order="40">
      </tab>
   </extension>
   <extension
         point="org.netxms.ui.eclipse.datacollection.dciOpenHandlers">
      <handler
            class="org.netxms.ui.eclipse.perfview.actions.ShowLineChart"
            priority="10">
      </handler>
      <handler
            class="org.netxms.ui.eclipse.perfview.actions.ShowTableLastValues"
            priority="20">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.netxms.ui.eclipse.perfview.commands.show_graph_filter"
            contextId="org.netxms.ui.eclipse.perfview.context.PredefinedGraphTree"
            schemeId="org.netxms.ui.eclipse.defaultKeyBinding"
            sequence="M1+F2">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.contexts">
      <context
            id="org.netxms.ui.eclipse.perfview.context.PredefinedGraphTree"
            name="Graphs"
            parentId="org.eclipse.ui.contexts.window">
      </context>
   </extension>

</plugin>
